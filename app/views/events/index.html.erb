<section class="events-section">
  <h1>Events</h1>

  <%= turbo_frame_tag "new_event" do %>
    <%= render "event_link" %>
  <% end %>

  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Event</th>
        <th scope="col">Location</th>
        <th scope="col">Date</th>
      </tr>
    </thead>
    <tbody id="events">
      <%= render @events %>
    </tbody>
  </table class="table">

</section>
